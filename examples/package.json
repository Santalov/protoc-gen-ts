    "@bazel/rollup": "latest",
    "@types/google-protobuf": "^3.15.0",
    "@types/node": "^14.14.41",
    "typescript": "^4.2.4",
    "http-server": "^14.0.0",
    "rollup": "^2.64.0",
    "@rollup/plugin-commonjs": "^21.0.1",
